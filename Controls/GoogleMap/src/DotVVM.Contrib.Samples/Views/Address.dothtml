@viewModel DotVVM.Contrib.Samples.ViewModels.AddressViewModel, DotVVM.Contrib.Samples
@masterPage Views/_master.dotmaster

    <dot:Content ContentPlaceHolderID="MainContent">
        <h1>Hardcoded</h1>
        <dc:GoogleMap Address="Chicken Dinner Rd Idaho 83607, USA" style="height:500px"  />

        <h1>Binding</h1>
        <dc:GoogleMap Address="{value: Address}" style="height:500px" />
        <dot:Button Text="Change address" Click="{command: ChangeAddress()}" />
    </dot:Content>

